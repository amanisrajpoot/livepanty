    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1490.0",
    "stripe": "^14.9.0",
    "nodemailer": "^6.9.7",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
